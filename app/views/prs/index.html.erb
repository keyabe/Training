<p id="notice"><%= notice %></p>

<h1>Prs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prs.each do |pr| %>
      <tr>
        <td><%= pr.title %></td>
        <td><%= link_to 'Show', pr %></td>
        <td><%= link_to 'Edit', edit_pr_path(pr) %></td>
        <td><%= link_to 'Destroy', pr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pr', new_pr_path %>
